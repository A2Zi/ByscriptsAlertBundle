{
    "name": "byscripts/alert-bundle",
    "description": "A flexible way to handle Symfony flash messages. Supports Bootstrap2, Bootstrap3 and Foundation out-of-the-box. Easily configurable to support any other UI framework.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Thierry Goettelmann",
            "email": "thierry@byscripts.info"
        }
    ],
    "require": {
        "symfony/dependency-injection": "~2.3.0",
        "symfony/config": "~2.3.0",
        "symfony/http-kernel": "~2.3.0",
        "symfony/http-foundation": "~2.3.0",
        "twig/twig": "~1.15.0"
    },
    "require-dev": {
        "symfony/framework-standard-edition": "~2.3.0"
    },
    "autoload": {
        "psr-4": {
            "Byscripts\\Bundle\\AlertBundle\\": "src/"
        }
    }
}
