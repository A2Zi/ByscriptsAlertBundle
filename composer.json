{
    "name": "byscripts/alert-bundle",
    "description": "A flexible way to handle Symfony flash messages. Supports Bootstrap2, Bootstrap3 and Foundation out-of-the-box. Easily configurable to support any other UI framework or custom CSS / Icons.",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Thierry Goettelmann",
            "email": "thierry@byscripts.info"
        }
    ],
    "require": {
        "symfony/dependency-injection": "~2.3",
        "symfony/config": "~3.4",
        "symfony/http-kernel": "~3.4",
        "symfony/http-foundation": "~3.4",
        "twig/twig": "~1.15"
    },
    "require-dev": {
        "symfony/framework-standard-edition": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Byscripts\\Bundle\\AlertBundle\\": "src/"
        }
    }
}
